{
  "name": "giphy-staff-yearbook-2023",
  "version": "1.0.0",
  "description": "GIPHY Yearbook 2023",
  "scripts": {
    "clean": "shx rm -rf .parcel-cache/* dist/*",
    "prebuild": "yarn run clean",
    "build": "parcel build ./public/index.html --dist-dir ./dist",
    "start": "parcel ./public/index.html"
  },
  "parcel": {
    "optimization": {
      "gzip": true
    },
    "extends": "@parcel/config-default",
    "plugins": {}
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.8.3",
    "@parcel/compressor-gzip": "^2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "buffer": "^5.5.0",
    "parcel-optimizer-friendly-urls": "^0.2.0",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "process": "^0.11.10",
    "serve": "^13.0.2"
  },
  "license": "MIT",
  "dependencies": {
    "@splinetool/runtime": "^0.9.257",
    "canvas-capture": "^2.0.6",
    "howler": "^2.2.3",
    "parcel": "^2.8.3",
    "scss-reset": "^1.2.2",
    "serve": "^13.0.2",
    "shx": "^0.3.4"
  }
}
